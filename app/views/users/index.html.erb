<%= render 'users/list', book: @book, user: current_user %>
<div class="col-xs-9">
<h2>User</h2>
<table class="table">
	<caption></caption>
	<thead>
		<tr>
			<th>image</th>
			<th>name</th>
			<th colspan="3"></th>
		</tr>
	</thead>
	<tbody>
		<% @users.each do |f| %>
		<tr>
			<td><%= attachment_image_tag @book.user, :profile_image, format: 'jpeg', fallback: "no_image.jpg",size:"40x40" %></td>
			<%= attachment_image_tag f, :profile_image %>
			<td><%= link_to f.name, user_path(f.id) %></td>
		</tr>
		<% end %>
	</tbody>
</table>
</div>